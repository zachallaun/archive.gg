{
  "name": "archive.gg",
  "description": "Archive.gg, a replay.gg archive",
  "author": "Zach Allaun <zach.allaun@gmail.com> (https://github.com/zachallaun)",
  "license": "MIT",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/zachallaun/archivegg"
  },
  "main": "babel.server.js",
  "scripts": {
    "start": "NODE_PATH=\"./src\" NODE_ENV=\"production\" node ./babel.server",
    "build": "node ./node_modules/webpack/bin/webpack.js --verbose --colors --display-error-details --config webpack/prod.config.js",
    "lint": "node ./node_modules/eslint/bin/eslint.js -c .eslintrc src",
    "start-dev": "NODE_PATH=\"./src\" NODE_ENV=\"development\" node ./babel.server",
    "watch-client": "UV_THREADPOOL_SIZE=100 NODE_PATH=\"./src\" node webpack/webpack-dev-server.js",
    "dev": "node ./node_modules/concurrently/src/main.js --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "async": "1.4.0",
    "babel": "5.4.7",
    "babel-plugin-typecheck": "0.0.3",
    "body-parser": "^1.13.2",
    "compression": "^1.5.0",
    "db-migrate": "0.9.18",
    "express": "^4.13.0",
    "file-loader": "^0.8.4",
    "http-proxy": "^1.11.1",
    "jquery": "2.1.4",
    "lodash": "3.10.0",
    "moment": "2.10.5",
    "piping": "0.1.8",
    "pg": "4.4.0",
    "react": "0.13.3",
    "react-classset": "0.0.2",
    "react-redux": "0.2.2",
    "react-router": "v1.0.0-beta2",
    "redux": "1.0.0",
    "redux-devtools": "1.0.2",
    "selectn": "0.9.6",
    "semantic-ui": "2.0.5",
    "serve-favicon": "^2.3.0",
    "serve-static": "^1.10.0",
    "sql": "0.55.0",
    "superagent": "^1.2.0",
    "url-loader": "^0.5.6",
    "react-zeroclipboard": "2.0.1",

    "autoprefixer-loader": "^2.0.0",
    "babel-core": "5.4.7",
    "babel-eslint": "^3.1.18",
    "babel-loader": "5.1.3",
    "babel-runtime": "5.4.7",
    "concurrently": "0.1.1",
    "dotenv": "1.2.0",
    "eslint": "^0.23.0",
    "eslint-config-airbnb": "0.0.6",
    "eslint-loader": "0.14.2",
    "eslint-plugin-react": "^2.5.2",
    "json-loader": "0.5.2",
    "node-sass": "^3.2.0",
    "react-hot-loader": "1.2.8",
    "strip-loader": "^0.1.0",
    "webpack": "^1.9.11",
    "webpack-dev-server": "1.9.0"
  },
  "engines": {
    "node": ">=0.10.32"
  }
}
